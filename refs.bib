@Article{Kitaura2016,
  author="Kitaura, Kazutaka
  and Shini, Tadasu
  and Matsutani, Takaji
  and Suzuki, Ryuji",
  title="A new high-throughput sequencing method for determining diversity and similarity of T cell receptor (TCR) α and β repertoires and identifying potential new invariant TCR α chains",
  journal="BMC Immunology",
  year="2016",
  volume="17",
  number="1",
  pages="38",
  abstract="High-throughput sequencing of T cell receptor (TCR) genes is a powerful tool for analyses of antigen specificity, clonality and diversity of T lymphocytes. Here, we developed a new TCR repertoire analysis method using 454 DNA sequencing technology in combination with an adaptor-ligation mediated polymerase chain reaction (PCR). This method allows the amplification of all TCR genes without PCR bias. To compare gene usage, diversity and similarity of expressed TCR repertoires among individuals, we conducted next-generation sequencing (NGS) of TRA and TRB genes in peripheral blood mononuclear cells from 20 healthy human individuals.",
  issn="1471-2172",
  doi="10.1186/s12865-016-0177-5",
  url="http://dx.doi.org/10.1186/s12865-016-0177-5"
}

@Article{Hung2016,
  author="Hung, Sheng-Jou
  and Chen, Yi-Lin
  and Chu, Chia-Hung
  and Lee, Chuan-Chun
  and Chen, Wan-Li
  and Lin, Ya-Lan
  and Lin, Ming-Ching
  and Ho, Chung-Liang
  and Liu, Tsunglin",
  title="TRIg: a robust alignment pipeline for non-regular T-cell receptor and immunoglobulin sequences",
  journal="BMC Bioinformatics",
  year="2016",
  volume="17",
  number="1",
  pages="433",
  abstract="T cells and B cells are essential in the adaptive immunity via expressing T cell receptors and immunoglogulins respectively for recognizing antigens. To recognize a wide variety of antigens, a highly diverse repertoire of receptors is generated via complex recombination of the receptor genes. Reasonably, frequencies of the recombination events have been shown to predict immune diseases and provide insights into the development of immunity. The field is further boosted by high-throughput sequencing and several computational tools have been released to analyze the recombined sequences. However, all current tools assume regular recombination of the receptor genes, which is not always valid in data prepared using a RACE approach. Compared to the traditional multiplex PCR approach, RACE is free of primer bias, therefore can provide accurate estimation of recombination frequencies. To handle the non-regular recombination events, a new computational program is needed.",
  issn="1471-2105",
  doi="10.1186/s12859-016-1304-2",
  url="http://dx.doi.org/10.1186/s12859-016-1304-2"
}

